{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [false, "always-prefix"],
    "member-access": false,
    "ordered-imports": false,
    "quotemark": [true, "single"]
  }
}
